{
  "rewrites": [
    {
      "source": "/manifest.webmanifest",
      "destination": "https://agendapp-backend.vercel.app/api/manifest.webmanifest"
    },
    {
      "source": "/favicon.ico",
      "destination": "https://agendapp-backend.vercel.app/api/favicon.ico"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
