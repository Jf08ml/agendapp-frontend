{
  "headers": [
    {
      "source": "/custom-sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/version.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/manifest.webmanifest",
      "destination": "https://agendapp-backend.vercel.app/api/manifest.webmanifest"
    },
    {
      "source": "/favicon.ico",
      "destination": "https://agendapp-backend.vercel.app/api/favicon.ico"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
